<h1>Listing trips</h1>

<table>
  <tr>
    <th>Stop</th>
    <th>1 url</th>
    <th>String</th>
    <th>Stop</th>
    <th>2 url</th>
    <th></th>
    <th>String</th>
    <th>Stop</th>
    <th>3 url</th>
    <th></th>
    <th>String</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @trips.each do |trip| %>
  <tr>
    <td><%= trip.Stop %></td>
    <td><%= trip.1_url %></td>
    <td><%= trip.string %></td>
    <td><%= trip.Stop %></td>
    <td><%= trip.2_url %></td>
    <td><%= trip. %></td>
    <td><%= trip.string %></td>
    <td><%= trip.Stop %></td>
    <td><%= trip.3_url %></td>
    <td><%= trip. %></td>
    <td><%= trip.string %></td>
    <td><%= link_to 'Show', trip %></td>
    <td><%= link_to 'Edit', edit_trip_path(trip) %></td>
    <td><%= link_to 'Destroy', trip, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

