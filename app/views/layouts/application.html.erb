<!DOCTYPE html>
<html>
<head>
  <title>Project1</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if alert %> 
	<div class="alert alert-error"> 
		<%= alert %>
	</div>
<% end %>

<% if notice %>
	<div class="notice notice-error">
		<%= notice %>
	</div>
<% end %>

<div class="container">
			<div id="help">
				<p>Welcome! Please fill in a username and pw. </p>
			</div>
			<button class="btn btn-inverse" type="button">Show/Hide Help Text</button>
			<%= yield %>
		</div>

</body>
</html>
